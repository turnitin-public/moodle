{{!
    @template core_ltix/tool_list

    This template provides a list of LTI tools.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    *

    Example context (json):
    {
    }

}}
<div id="tool-card-container">
    {{^proxies}}{{^tools}}
        <p>{{#str}} no_lti_tools, core_ltix {{/str}}</p>
    {{/tools}}{{/proxies}}
    {{#proxies}}
        {{> core_ltix/tool_proxy_card }}
    {{/proxies}}
    {{#tools}}
        {{> core_ltix/tool_card }}
    {{/tools}}
</div>
